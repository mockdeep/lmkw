%h1 Log in to YourAppNameHere

= form_with(scope: :session, url: session_path) do |form|
  .field
    = form.label(:email)
    = form.email_field(:email, required: true)

  .field
    = form.label(:password)
    = form.password_field(:password, required: true)
  .actions
    = form.submit("Log In")
